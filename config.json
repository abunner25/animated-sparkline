      "elements": [
        {
          "id": "date",
          "label": "Date",
          "type": "DIMENSION",
          "options": {
            "min": 1,
            "max": 1,
            "supportedTypes": ["TIME"]
          }
        },
        {
          "id": "value",
          "label": "Metric",
          "type": "METRIC",
          "options": {
            "min": 1,
            "max": 1
          }
        }
      ]
    }
  ],
  "style": [
    {
      "id": "sparklineStyle",
      "label": "Sparkline style",
