        {
          "id": "lineColor",
          "label": "Line color",
          "type": "COLOR",
          "defaultValue": "#7E57C2"
        },
        {
          "id": "lineWidth",
          "label": "Line thickness (px)",
          "type": "NUMBER",
          "defaultValue": 3
        },
        {
          "id": "durationMs",
          "label": "Animation duration (ms)",
          "type": "NUMBER",
          "defaultValue": 900
